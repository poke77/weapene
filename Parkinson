game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
    Text = "Copied New Discord Server",
    Font = Enum.Font.SourceSansBold,
    TextSize = 20,
})

setclipboard("https://discord.gg/6hNUHuVQD5")
game.StarterGui:SetCore("SendNotification", {
Title = "script.gg";
Text = "Discord Server -> https://discord.gg/6hNUHuVQD5";
Icon = "";
Duration = 5;
})

local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
  vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
  wait(1)
  vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

local ScriptURL = "https://raw.githubusercontent.com/Kederal/script.gg/main/SupportedGames/"

local SupportedGames = {
    [3102144307] = 'AnimeClickerSimulator';
    [7560156054] = 'ClickerSimulator';
    [8592863835] = 'MagnetSimulator2';
    [8554378337] = 'WeaponFightingSimulator';
    [8540346411] = 'RebirthChampionsX';
}

for i,v in next, SupportedGames do
if i == game.PlaceId or i == game.GameId then
loadstring(game:HttpGet(ScriptURL .. v .. '.lua'))()       
end
end
